<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingCardView">
        <attr name="loadingBackgroundColor" format="reference|color" />
        <attr name="loadingCardTheme" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="TagCardView">
        <attr name="tagCardColor" format="reference|color" />
        <attr name="tagTextColor" format="reference|color" />
        <attr name="tagIcon" format="reference|integer" />
        <attr name="tagCardTheme" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="IconCardView">
        <attr name="titleBackgroundColor" format="reference|color" />
        <attr name="detailBackgroundColor" format="reference|color" />
        <attr name="titleTextColor" format="reference|color" />
        <attr name="detailTextColor" format="reference|color" />
        <attr name="headerIcon" format="reference|integer" />
        <attr name="iconCardTheme" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="LiveCardView">
        <attr name="liveCardColor" format="reference|color" />
        <attr name="liveTextColor" format="reference|color" />
        <attr name="liveCardTheme" format="reference|integer" />
    </declare-styleable>
</resources>